{
  "name": "twofa-authenticator",
  "version": "1.3.1",
  "description": "This extension provides 2FA authentication codes in the browser",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hemancini/2fa-authenticator.git"
  },
  "scripts": {
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch --mode dev",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "test": "jest",
    "package": "zip -r $MV3_NAME.$MV3_VERION.zip dist",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --loglevel warn --write \"./src/**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.5",
    "crypto-js": "4.1.1",
    "framer-motion": "10.12.17",
    "jsqr": "1.4.0",
    "qrcode.react": "3.1.0",
    "react": "18.2.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.0",
    "superjson": "^2.2.1",
    "wouter": "2.11.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.5.0",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.239",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "29.0.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.2.6",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.2.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "fs-extra": "11.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rollup": "2.79.1",
    "sass": "1.62.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.4",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "ws": "8.13.0"
  }
}
